#
# For a description of the syntax of this configuration file,
# see the file kconfig-language.txt in the NuttX tools repository.
#

config CURUVAR_MOTOR_CONTROL
	tristate "Curuvar MotorController"
	default n

if CURUVAR_MOTOR_CONTROL

config CURUVAR_MOTOR_CONTROL_PROGNAME
	string "Program Name"
	default "motor_control"
	---help---
		This is the name of the program that will be used when the NSH ELF
		program is installed.

config CURUVAR_MOTOR_CONTROL_PRIORITY
	int "Task Priority"
	default 100

config CURUVAR_MOTOR_CONTROL_STACKSIZE
	int "Stack Size"
	default DEFAULT_TASK_STACKSIZE

endif
